    <string name="sql_select_buslines">select busline.id as _id,busline.num || \' \' || busline.name_gr as linestr from busline</string>
    <string name="sql_select_stops">select v_stop.stopid as stopid ,v_stop.sorder ||\'.
		\' || v_stop.stopname_gr as stopstr from v_stop where v_stop.direction
		= ? and v_stop.lineid = ?</string>
    <string name="sql_select_linespassing">select distinct v_linepass.busnum as busnum,
		v_linepass.busname_gr as busname from v_linepass where
		v_linepass.stopid = ?</string>
